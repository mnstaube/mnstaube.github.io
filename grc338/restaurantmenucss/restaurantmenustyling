body {
	background-color: rgb(250, 250, 250); /* Not totally white. */
	color: ; rgb(48, 52, 62) /* Not totally black. */
	line-height: 140%; 	/* A little extra leading. */
	font-family: san-serif;
}

h1 {
	background-color: rgb(153, 153, 169);
	color: rgb(250, 250, 250); /* Same as body background. */
	font-size: 10;
	padding: 10, 10, 10; /* You want three values. */
	font-weight: 4;
	text-transform: uppercase;
}

